{
    "name": "teamprodev/laravel-payment",
    "description": "Payment package based on GoodOneUz payment package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Teamprodev\\LaravelPayment\\": "src/",
            "Teamprodev\\LaravelPayment\\database\\seeds\\": "src/database/seeds"
        }
    },
    "authors": [
        {
            "name": "Faxriddin077",
            "email": "faxriddinqodirov077@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/support": "5.7.*|5.8.*|6.*|7.*|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Teamprodev\\LaravelPayment\\PayUzServiceProvider"
            ],
            "aliases": {
                "PayUz": "Teamprodev\\LaravelPayment\\PayUzFacade"
            }
        }
    }
}
